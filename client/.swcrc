{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "target": "es2015",
    "transform": {
      "react": {
        "runtime": "automatic",
        "refresh": true
      }
    },
    "parser": {
      "syntax": "typescript"
    }
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "env": {
    "mode": "usage",
    "coreJs": 3.25
  }
}
